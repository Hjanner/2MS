{
  "name": "2ms-frontend",
  "description": "Morela's Management System (2MS) está diseñado para automatizar y optimizar los procesos clave del Cafetin Morela.",
  "author": "Hanner Hernandez <hjanner918@email.com, Github: Hjanner> | Carlos Gomez <>",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "start": "electron .",
    "db:init": "python database/create_db.py && python database/insert_db.py && python database/triggers_db.py && python database/views_db.py"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.6",
    "@mdi/font": "7.4.47",
    "axios": "^1.11.0",
    "vue": "^3.5.17",
    "vuetify": "^3.9.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "concurrently": "^9.2.0",
    "electron": "^37.3.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.30.1",
    "eslint-config-vuetify": "^4.0.0",
    "sass-embedded": "^1.89.2",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.8.0",
    "unplugin-vue-router": "^0.14.0",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-router": "^4.5.1",
    "wait-on": "^8.0.4"
  }
}
